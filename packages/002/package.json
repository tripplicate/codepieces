{
  "name": "@piece/002",
  "packageManager": "pnpm@8.15.5",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type:check && vite build",
    "preview": "vite preview",
    "type:check": "vue-tsc --noEmit",
    "lint:eslint": "eslint '**/*.{js,jsx,cjs,mjs,ts,tsx,vue}' --fix --cache --cache-strategy=content --cache-location=.temp/.eslintcache --ignore-path=.gitignore",
    "lint:stylelint": "stylelint './src/**/*.css' --fix --allow-empty-input --cache --cache-strategy=content --cache-location=.temp/.stylelintcache --ignore-path=.gitignore",
    "lint:all": "pnpm lint:eslint && pnpm lint:stylelint",
    "bundle-visualizer": "pnpm exec vite-bundle-visualizer -o .temp/bundle-stats.html"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@unhead/vue": "^1.8.20",
    "@vueuse/core": "^10.9.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@trpl/eslint-config": "^1.3.0",
    "@trpl/stylelint-config-standard": "^1.2.0",
    "@tsconfig/node21": "^21.0.3",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "postcss-nested": "^6.0.1",
    "stylelint": "^16.2.1",
    "typescript": "~5.4.3",
    "vite": "^5.2.7",
    "vite-bundle-visualizer": "^1.1.0",
    "vite-plugin-compression2": "^1.0.0",
    "vue-tsc": "^2.0.6"
  }
}
